         <module>audiobus</module>
      </modules>
      <code.krate><![CDATA[const int32_t *d = audiobus_get(attr_bus-1);
int i;
for(i=0;i<BUFSIZE;i++){
	outlet_out[i] = d[i];
}]]></code.krate>
   </obj.normal>
